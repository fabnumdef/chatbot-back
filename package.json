{
  "name": "chatbot-back",
  "version": "1.9.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy": "cp .env dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migrate": "yarn typeorm migration:generate -d src/data-source.ts",
    "typeorm:run": "yarn typeorm migration:run",
    "artillery": "artillery run test/artillery/load-test.yaml --output test/artillery/report.json",
    "artillery:report": "artillery report test/artillery/report.json -o test/artillery/report.html"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
