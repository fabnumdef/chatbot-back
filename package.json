{
  "name": "chatbot-back",
  "version": "1.7.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy": "cp .env dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "artillery": "artillery run test/artillery/load-test.yaml --output test/artillery/report.json",
    "artillery:report": "artillery report test/artillery/report.json -o test/artillery/report.html"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/typeorm": "~8.0.2",
    "ansible-playbook-cli-js": "^2.0.5",
    "archiver": "^4.0.1",
    "bcrypt": "^5.0.1",
    "camelcase-keys": "^6.2.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.1",
    "get-folder-size": "^2.0.1",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.3",
    "nestjs-typeorm-paginate": "^3.2.1",
    "nodemailer": "^6.6.1",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pg-escape": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.1.0",
    "snakecase-keys": "^3.1.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.38",
    "uuid": "^7.0.3",
    "xlsx": "^0.17.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.2",
    "@types/jest": "26.0.4",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "artillery": "^1.7.9",
    "artillery-engine-socketio-v3": "^1.1.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^29.0.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^29.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
